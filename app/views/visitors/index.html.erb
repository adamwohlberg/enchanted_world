    <!--     # mobile unicorn -->
    <div id="mobile-form" style="width: 100%; margin-top: 50px;">
      <h4 id="tap-to-find-unicorns" style="color: white; width: 99%; position: relative;">Tap to find a unicorn</h4><br>
      <h4 id="long-tap-to-find-unicorns" style="color: white; width: 99%; position: relative;">Long tap to <br>turn on unicorn finder</h4><br>
      <%= image_tag('unicorn-example.png', id: 'mobile-unicorn-button', class: 'hoverReveal pulse-button') %>
    <form id="cell-number-input">
      <p style="font-size: 14px; color: white;">we'll text you here when a unicorn is nearby:</p>
      <input style="margin-top: -30px; width: 300px;" type="tel" name="usrtel" placeholder="310.555.1212">
    </form>
